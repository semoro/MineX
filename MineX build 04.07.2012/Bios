arg={...}local d=arg[1] or 1  term.clear()term.setCursorPos( 1, 1 )print("BIOS !1.00!V")sleep(d)print("_")sleep(d)term.setCursorPos( 1, 2 )term.clearLine()print(" _")sleep(d)term.setCursorPos( 1, 2 )term.clearLine()sleep(d)print("Finding SATA HDD ROM")sleep(d)print("ROM HDD is find")local mass=rs.getSides()drive={}modem={}for n=1,#mass doif peripheral.getType(mass[n])=="drive" thendrive[n]=peripheral.getType(mass[n])print("Disk drive found at "..mass[n])endif peripheral.getType(mass[n])=="modem" thenmodem[n]=peripheral.getType(mass[n])print("Wireless modem found at "..mass[n])endendsleep(d)print("ROOT is find")sleep(d)print("MineX allocated!")sleep(d)print("BOOT MineXstart(-S-ALPHA-)!")sleep(d)term.clear()term.setCursorPos( 1, 1 )dofile("MineXstart")